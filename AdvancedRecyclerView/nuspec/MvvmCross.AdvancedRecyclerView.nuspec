<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>MvvmCross.AdvancedRecyclerView</id>
    <version>2.1.0.4</version>
    <title>MvvmCross.Bindings.AdvancedRecyclerView</title>
    <authors>Przemyslaw Raciborski [thefex]</authors>
    <projectUrl>https://github.com/thefex/Xamarin.Bindings.AdvancedRecyclerView</projectUrl>
    <licenseUrl>https://github.com/thefex/Xamarin.Bindings.AdvancedRecyclerView/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>
        Mvx Wrapper for AdvancedRecyclerView (1.0.0 custom - with support of RecyclerView 1.2.14) library.
        Currently supports:
        - grouping,
        - swipe,
        - header/footer
    </description>
    <tags>xamarin android mvvmcross advancedrecyclerview grouping swipe</tags>
    <dependencies>
      <group targetFramework="monoandroid12.0">
          <dependency id="MvvmCross.DroidX.RecyclerView" version="8.0.2"/>
          <dependency id="Xamarin.AndroidX.RecyclerView" version="1.2.1.7"/>
          <dependency id="Xamarin.Bindings.AdvancedRecyclerView" version="2.1.0.1"  />
        </group>
	<group targetFramework="monoandroid10.0">
		<dependency id="MvvmCross.DroidX.RecyclerView" version="7.1.2"/>
		<dependency id="Xamarin.Bindings.AdvancedRecyclerView" version="2.1.0.1"/>
		<dependency id="Microsoft.Extensions.Logging.Abstractions" version="5.0.0"/>
	</group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\MvvmCross.AdvancedRecyclerView\bin\Release\MvvmCross.AdvancedRecyclerView.*" target="lib\MonoAndroid" />
  </files>
</package>













